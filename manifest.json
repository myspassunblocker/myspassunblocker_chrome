{
  "name": "MySpass Adblocker",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Diese Erweiterung blockiert die l√§stigen Werbeeinblendungen auf Myspass.de (blockiert auch die adblock Warnung).",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "de",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "MySpass Adblocker",
    "default_popup": "src/page_action/page_action.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.myspass.de/*"
      ],
      "js": [
		"js/swfobject.js",
		"js/jquery/jquery.min.js",
		"js/player.js",
        "src/inject/inject.js"
      ]
    }
  ]
}